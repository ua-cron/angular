{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "npm run clean && npm run build ng-cron && node prepare-package.mjs",
    "build-lib-alpha": "ALPHA=1 npm run build-lib",
    "clean": "rm -rf dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npm run build-lib && npm run e2e-inject && npm run build e2e && cypress open --component",
    "e2e-run": "npm run build-lib && npm run e2e-inject && npm run build e2e && cypress run --component",
    "e2e-inject": "npm install ./dist/ng-cron --force",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && ng run ng-cron:storybook",
    "build-storybook": "npm run docs:json && ng run ng-cron:build-storybook",
    "publish-alpha": "npm publish ./dist/ng-cron --tag alpha --access=public",
    "publish": "npm publish ./dist/ng-cron --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@sbzen/cron-core": "2.0.2",
    "@sbzen/ng-cron": "file:dist/ng-cron",
    "bootstrap": "^5.3.0",
    "bootstrap-legacy": "npm:bootstrap@^4.6.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.2",
    "@babel/core": "^7.25.2",
    "@compodoc/compodoc": "^1.1.25",
    "@sbzen/e2e": "https://github.com/ua-cron/e2e/releases/download/20230106035216/dist.tar.gz",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/angular": "^8.2.9",
    "@storybook/builder-webpack5": "^8.2.9",
    "@storybook/testing-library": "^0.2.2",
    "@types/jasmine": "~5.1.4",
    "@types/node": "~22.5.2",
    "babel-loader": "^9.1.3",
    "cypress": "^13.14.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.1",
    "typescript": "~5.5.4"
  }
}
